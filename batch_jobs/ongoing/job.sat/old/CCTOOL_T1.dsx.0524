#################################################################
#### STAGE: odbc1
## Operator
odbcread
## Operator options
-db_cs [&DSProjectMapName]
-data_source 'vm2k3'
-user 'cctool'
-password [&__V0S0P1_password]
-query 'SELECT 
  context, agent_context, operation, transactionID, tableName, rowID, timestamp, COL1, COL2
FROM 
CCTOOL_T1
where context >
(select context from table_context
where tablename = \\'CCTOOL_T1\\' )'
