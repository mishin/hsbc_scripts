## General options
[ident('odbc1'); jobmon_ident('odbc1')]
## Outputs
0> [modify (
  context:not_nullable string[32]=context;
  agent_context:not_nullable string[64]=agent_context;
  operation:not_nullable string[12]=operation;
  transactionID:not_nullable string[24]=transactionID;
  tableName:not_nullable string[64]=tableName;
  rowID:not_nullable string[max=24]=rowID;
  timestamp:not_nullable string[26]=timestamp;
  COL1:nullable string[max=10]=COL1;
  COL2:nullable string[max=10]=COL2;
keep
  context, agent_context, operation, transactionID, tableName, rowID, timestamp, COL1, COL2;
)] 'odbc1:link1.v'
;

